    <tei:zone xml:id="bmst8.ln.idp19962368" type="line" ulx="736" uly="295" lrx="1362" lry="343">
      <tei:line>VOORREDEN.</tei:line>
    </tei:zone>
    <tei:zone xml:id="bmst8.ln.idp15175008" type="line" ulx="595" uly="384" lrx="1589" lry="443">
